<h1>My Celebrities</h1>
<a href='/celebrities/new'>Create a new Celebrity</a>
<ul>
{{#each myCelebrities}}
  <li>
    <a href ='/celebrities/{{_id}}'>{{name}}</a>
    <form action="/celebrities/{{_id}}/delete" method="POST">
      <button>Delete</button>
    </form>
    <a href = '/celebrities/{{_id}}/edit'>Edit</a>
  </li>
{{/each}}
</ul>